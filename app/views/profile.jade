!!!
html
	head
		title profile
		script(type='text/javascript', src='/cdn/json2.js')
		script(type='text/javascript', src='/cdn/jquery-min.js')
		script(type='text/javascript', src='/cdn/underscore-min.js')
		script(type='text/javascript', src='/cdn/backbone-min.js')
		script(type='text/javascript', src='js/hump.js')
		link(rel='stylesheet', type='text/css', media='all', href='css/hump.css')
	body
		div#profile
		hr

		div#activities

		script(type='text/template', id='profile-template')
			<% if (_id) { %>
			<!--div><%= _id %></div-->
			<div style='float: left'><img src='<%= dp %>' width='100' height='100'/></div>
			<div><b>name: </b><%= name %></div>
			<div class='about'><b>about: </b><%= about %></div>
			<% } %>

		script(type='text/template', id='activity-template')
			<% if (camel) { %>
			<!--div><%= camel %></div-->
			<div><%= log%></div>
			<% } %>
